type Query {
 getBookByTitle(title: String): [BookDTO]
 getBookByPublisherAndPrice(publisher: String, price: Float): [BookDTO]
}

type Mutation {
 saveBook(book: BookDTOInput): BookDTO
 deleteBook(id: ID): BookDTO
}

type BookDTO {
 title: String
 publisher: String
 datePublication: String
 price: Float
}

input BookDTOInput {
 title: String
 publisher: String
 datePublication: String
 price: Float
}
